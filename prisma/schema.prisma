generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Account {
  id String @id @default(uuid())
  accountNumber Int
  name String
  amount Int @default(0)
  accountType String
  creditLimit Int @default(0)
  createdAT DateTime @default(now())
  updatedAT DateTime @updatedAt
  transType String?
  transCount Int?
}
